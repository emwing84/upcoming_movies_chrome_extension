{
  "manifest_version": 3,
  "name": "Upcoming Movies",
  "description": "This is an extension to look up movies coming to threaters",
  "version": "1.0.0",
  "author": "Jeff Y",
  "icons": {
    "16": "/images/icon16.png",
    "48": "/images/icon48.png",
    "128": "/images/icon128.png"
  },
  "action": {
    "default_icon": {
        "16": "/images/movie16.png",
        "48": "/images/movie48.png",
        "128": "/images/movie128.png"
    },
    "default_popup": "index.html",
    "default_title": "Upcoming Movies"
  }
}